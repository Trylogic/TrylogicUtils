<?xml version="1.0" encoding="UTF-8"?><ivysettings>	<settings defaultResolver="chained" />	<resolvers>		<chain name="chained" returnFirst="true">			<url name="github" m2compatible="false">				<artifact pattern="https://github.com/[organisation]/[module]/raw/master/repo/[revision]/[artifact]-[revision].[ext]" />			</url>		</chain>		<filesystem name="local" m2compatible="false" force="false" local="true">			<artifact pattern="${basedir}/repo/[revision]/[artifact]-[revision].[ext]" />		</filesystem>	</resolvers></ivysettings>